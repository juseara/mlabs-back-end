{
  "name": "mlabs-teste-back-end",
  "version": "1.0.0",
  "description": "Teste banck-end para mlabs",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && babel src -d dist",
    "now-build": "npm run build",
    "clean": "rimraf dist",
    "db": "babel-node ./db/index.js",
    "dev": "npm run db && nodemon -L --exec babel-node ./src/index.js",
    "start": "npm run clean && npm run build && npm run db && cd dist && node index.js",
    "test": "jest src --watch",
    "test:coverage": "jest --coverage src",
    "docker-dev": "docker-compose -f docker-compose.yml -f docker-development.yml up -d"
  },
  "keywords": [],
  "author": "Jos√© Augusto",
  "license": "ISC",
  "jest": {
    "testPathIgnorePatterns": [
      "dist"
    ],
    "verbose": false,
    "globalSetup": "./jest-setup.js",
    "globalTeardown": "./jest-teardown.js",
    "testEnvironment": "./jest-mongo.js"
  },
  "dependencies": {
    "babel-plugin-inline-dotenv": "^1.5.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cuid": "^2.1.6",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "mongodb": "^3.3.2",
    "sanitize-html": "^1.20.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/preset-env": "^7.6.3",
    "babel-jest": "^24.9.0",
    "dotenv": "^8.1.0",
    "faker": "^4.1.0",
    "jest": "^24.9.0",
    "mongodb-memory-server": "^5.2.8",
    "nodemon": "^1.19.3",
    "rimraf": "^3.0.0"
  }
}
